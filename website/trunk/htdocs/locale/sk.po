#~ "<tr> <td> <code> (/ AB) </code> </td> <td> delenie </td> </tr> \n"
#~ "<tr> <td> <code> (truncate ab) </code> </td> <td>zaokrúhľovať nadol na "
#~ "celé číslo (floor) </td> </tr>\n"
#~ "<tr> <td> <code> (float ab) </code> </td> <td> integer na plávajúci bod </"
#~ "td> </tr>\n"
#~ "<tr> <td> <code> (rem abc ...)</code> </td> <td> zvyšok </td> </tr>\n"
#~ "<tr> <td> <code> (min abc ...)</code> </td> <td> minimálne </td> </tr>\n"
#~ "<tr> <td> <code> (max abc ...)</code> </td> <td> maximálne </td> </tr>\n"
#~ "<tr> <td> <code> (abs a) </code> </td> <td> absolútna hodnota </td> </"
#~ "tr>\n"
#~ "<tr> <td> <code> (random n) </code> </td> <td> náhodného medzi 1 a n-1 </"
#~ "td> </tr>\n"
#~ "<tr> <td> <code> (sin ab) </code> </td> <td> sínus </td> </tr>\n"
#~ "<tr> <td> <code> (cos ab) </code> </td> <td> kosínus </td> </tr>\n"
#~ "<tr> <td> <code> (tan ab) </code> </td> <td> tangens </td> </tr>\n"
#~ "<tr> <td> <code> (expt ab) </code> </td> <td> exponent (a to moc b) </td> "
#~ "</tr>\n"
#~ "<tr> <td> <code> (sqrt ab) </code> </td> <td> odmocnina </td> </tr>\n"
#~ "<tr> <td> <code> (<a b) </code> </td> <td> test  a menej ako b </td> </"
#~ "tr>\n"
#~ "<tr> <td> <code> (<= ab) </code> </td> <td> test a menej ako alebo sa "
#~ "rovná b </td> </tr>\n"
#~ "<tr> <td> <code> (> ab) </code> </td> <td> test a väčšie ako b </td> </"
#~ "tr>\n"
#~ "<tr> <td> <code> (> = ab) </code> </td> <td> test a väčšie alebo sa rovná "
